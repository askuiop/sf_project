<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="apple-touch-icon" href="apple-touch-icon.png">
        <link rel="stylesheet" href="{{ asset("bundles/jimspete/css/bootstrap.min.css") }}">

        {% block stylesheets %}
            {% stylesheets
            "bundles/jimspete/css/bootstrap.min.css"
            "@JimsPeteBundle/Resources/public/css/bootstrap-theme.min.css"
            "@JimsPeteBundle/Resources/public/css/main.css"
            %}
            <link rel="stylesheet" href="{{ asset_url }}">
            {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        <style>
            body {
                padding-top: 50px;
                padding-bottom: 20px;
            }
        </style>
    </head>
    <body>
        {% block body %}{% endblock %}


        {% block javascripts %}
            <script src="{{ asset('bundles/jimspete/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js') }}"></script>

            <script>window.jQuery || document.write('<script src="{{ asset('bundles/jimspete/js/vendor/jquery-1.11.2.min.js') }}"><\/script>')</script>

            <script src="{{ asset('bundles/jimspete/js/vendor/bootstrap.min.js') }}"></script>

            <script src="{{ asset('bundles/jimspete/js/plugins.js') }}"></script>
            <script src="{{ asset('bundles/jimspete/js/main.js') }}"></script>


        {% endblock %}
    </body>
</html>
