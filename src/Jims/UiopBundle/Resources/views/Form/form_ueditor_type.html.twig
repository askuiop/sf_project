{% block ueditor_widget %}
    <script id="container" {{ block('widget_attributes') }} type="text/plain">{% if value is not empty %} {{ value|raw }} {% endif %}</script>
    <!-- 配置文件 -->
    <script type="text/javascript" src="{{ asset('bundles/jimsuiop/ueditor.config.js') }}"></script>
    <!-- 编辑器源码文件 -->
    <script type="text/javascript" src="{{ asset('bundles/jimsuiop/ueditor.all.js') }} "></script>
    <!-- 实例化编辑器 -->
    <script type="text/javascript">
        var ue = UE.getEditor('container');
    </script>
{% endblock ueditor_widget %}